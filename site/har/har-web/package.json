{
  "name": "har-cbd-project-web",
  "description": "MIS平台",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "api": "ytt",
    "dev": "yarn dev:cube",
    "dev:demo": "cross-env MODE=demo vite --host --config ./config/vite.config.dev.ts",
    "dev:cube": "cross-env MODE=cube vite --host --config ./config/vite.config.dev.ts",
    "report": "cross-env REPORT=true npm run build",
    "lint-staged": "npx lint-staged",
    "build": "run-s type-check build-only:cube",
    "build:demo": "run-s type-check build-only:demo",
    "build-only:cube": "cross-env MODE=cube yarn build-only",
    "build-only:demo": "cross-env MODE=demo yarn build-only",
    "build-only": "vite build --config ./config/vite.config.prod.ts",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.vue": [
      "stylelint --fix",
      "prettier --write",
      "eslint --fix"
    ],
    "*.{less,css}": [
      "stylelint --fix",
      "prettier --write"
    ]
  },
