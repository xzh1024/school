@import "../starTemplate/starTemplate.wxss";

.movie-container{
  width: 30%;
  height: 400rpx;
  /* border: 1px solid rgb(23, 148, 231); */
  display: flex;
  flex-direction: column;
  align-items: center;
  overflow: hidden;
  margin-bottom: 20rpx;
  background-color: #f0f0f0;
}
.movie-img{
  width: 100%;
  height: 300rpx;
}
.movie-name{
  width: 100%;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  margin-top: 10rpx;
}